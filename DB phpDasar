mysql> use phpDasar;
Database changed
mysql> create table Mahasiswa(
    -> id int primary key auto_increment,
    -> nama varchar(100),
    -> nis char(5),
    -> email varchar(100),
    -> jurusan varchar(100),
    -> gambar varchar(100));

mysql> desc mahasiswa;
+---------+--------------+------+-----+---------+----------------+
| Field   | Type         | Null | Key | Default | Extra          |
+---------+--------------+------+-----+---------+----------------+
| id      | int          | NO   | PRI | NULL    | auto_increment |
| nama    | varchar(100) | YES  |     | NULL    |                |
| nis     | char(5)      | YES  |     | NULL    |                |
| email   | varchar(100) | YES  |     | NULL    |                |
| jurusan | varchar(100) | YES  |     | NULL    |                |
| gambar  | varchar(100) | YES  |     | NULL    |                |
+---------+--------------+------+-----+---------+----------------+

mysql> insert into mahasiswa(nama, nis, email, jurusan, gambar) values(
    -> 'Alvaro Zidane',
    -> '16893',
    -> 'shuuu@gmail.com',
    -> 'Psikologi',
    -> 'ALVARO ZIDANE_FOTO MERAH.jpg');

